{
  "name": "react-sortly",
  "version": "1.2.1",
  "description": "React nested sortable component",
  "author": "",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lytc/react-sortly.git"
  },
  "keywords": [
    "react",
    "nested",
    "sortable",
    "tree",
    "drag",
    "drop",
    "dnd"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lytc/react-sortly/issues"
  },
  "homepage": "https://github.com/lytc/react-sortly#readme",
  "scripts": {
    "start": "node ./server.js",
    "flow": "flow",
    "lint": "$(npm bin)/eslint src docs/app",
    "build": "better-npm-run build:lib && better-npm-run build:dist && better-npm-run build:finally",
    "test": "$(npm bin)/istanbul cover --report text --report html $(npm bin)/_mocha -- --require babel-core/register",
    "prepublishOnly": "npm run build"
  },
  "betterScripts": {
    "build:lib": {
      "command": "rm -rf lib && $(npm bin)/babel src --source-maps --out-dir lib",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dist": {
      "command": "rm -rf dist && $(npm bin)/webpack -p --progress --config webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:finally": "mv dist/app.js docs/ && mv dist/app.js.map docs/"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": ">=15.0.0",
    "react-dnd": "*"
  },
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "7",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "flow-bin": "^0.83.0",
    "flow-runtime": "^0.17.0",
    "highlight.js": "^9.13.1",
    "istanbul": "^1.1.0-alpha.1",
    "lodash.debounce": "^4.0.8",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.4",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-json-view": "^1.14.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^7.1.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.21.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hmr": "^1.0.2"
  },
  "dependencies": {
    "immutability-helper": "^2.6.4"
  }
}
